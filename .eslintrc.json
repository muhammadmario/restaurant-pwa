{
  "env": {
    "browser": true,
    "es2021": true,
    "es6": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": "off",
    "no-restricted-globals": "off",
    "import/first": "off",
    "array-callback-return": "off"
  },
  "plugins": ["prettier"]
}
